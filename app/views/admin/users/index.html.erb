<h2>ユーザー一覧</h2>

<table>
	<tr>
		<th>ユーザー名(カナ)</th>
		<th>ユーザー名　(漢字)</th>
		<th>e-mail</th>
		<th>電話番号</th>
		<th>編集</th>

	</tr>
		<% @users.each do |user| %>
		<tr>
			<td><%= user.first_name_kana + user.last_name_kana %> </td>
			<td> <%= link_to user.first_name + user.last_name, admin_user_path(user) %> </td>
			<td> <%= user.email %> </td>
			<td> <%= user.phone_number %> </td>
			<td> <%= link_to "編集", edit_admin_user_path(user) %> </td>
			<%end%>